  90:Core/Src/lcd_stm32f0.c ****     else				// else RESET D6
  91:Core/Src/lcd_stm32f0.c ****     {
  92:Core/Src/lcd_stm32f0.c ****         GPIOA->BSRR |= LCD_D6_RESET;
  93:Core/Src/lcd_stm32f0.c ****     }
  94:Core/Src/lcd_stm32f0.c **** // DATALINE 5
