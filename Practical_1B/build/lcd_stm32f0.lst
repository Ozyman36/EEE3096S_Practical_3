 143:Core/Src/lcd_stm32f0.c ****     if ((command & 0x01) != 0)		// Select bit 0 of command, if HIGH set Data line 4 (D4)
 144:Core/Src/lcd_stm32f0.c ****     {
 145:Core/Src/lcd_stm32f0.c ****     	GPIOB->BSRR |= LCD_D4_SET;
